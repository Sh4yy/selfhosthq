<script is:inline>
  function copyUrl() {
    console.log(navigator.clipboard);
    if (navigator.clipboard && navigator.clipboard.writeText) {
      console.log("copying");
      navigator.clipboard.writeText(window.location.href);
    } else {
      console.error("Clipboard API not supported");
    }
  }
</script>

